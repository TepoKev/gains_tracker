generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  password String
  firstName String
  lastName String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}